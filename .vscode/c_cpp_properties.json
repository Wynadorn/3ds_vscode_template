{
    "configurations": [
        {
            "name": "3DS",
            "forcedInclude": ["${workspaceFolder}/include/vscode_fix.h"],
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include/**",
                "${DEVKITPRO}/libctru/include/**",
                "${DEVKITPRO}/devkitARM/arm-none-eabi/include/**"
            ],
            "defines": [
                "_DEBUG",
                "_UNICODE",
                "WIN32",
                "ARM9",
                "ARM11"
            ],
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${workspaceFolder}/include/**",
                    "${DEVKITPRO}/libctru/include/**",
                    "${DEVKITPRO}/devkitARM/arm-none-eabi/include/**",
                    "${DEVKITPRO}/portlibs/3ds/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++11",
            "compilerPath": "${DEVKITPRO}/devkitARM/bin/arm-none-eabi-g++.exe"
        }
    ],
    "version": 4
}